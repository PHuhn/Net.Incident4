@model NSG.NetIncident4.Core.Application.Commands.NoteTypes.NoteTypeDetailQuery
@{
    ViewData["Title"] = "Delete";
}
<h2>Note Type (Delete)</h2>
<div class="col-md-9">
    <hr />
    <form asp-controller="NoteTypes" asp-action="Delete" asp-route-id="@(Model.NoteTypeId)" method="post">
        <div asp-validation-summary="All" class="text-danger"></div>
        @Html.AntiForgeryToken()
        <div class="form-actions no-color">
            <div class="nsg-float-left text-left">
                <span>
                    @Html.ActionLink("Back to List", "Index")
                </span>
            </div>
            <div class="nsg-float-right text-right">
                <span>
                    <strong>Are you sure you want to delete this Note Type?</strong>
                    <input type="submit" value="Delete" class="btn btn-danger" />
                </span>
            </div>
            <div class="nsg-clear-both"></div>
        </div>

        @await Html.PartialAsync("NoteTypeDetailPartial", @Model)

    </form>

</div>
