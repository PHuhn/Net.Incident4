@model NSG.NetIncident4.Core.Application.Commands.ApplicationUsers.ApplicationUserDetailQuery
@{
    ViewData["Title"] = "Delete";
}
<h2>User (Delete)</h2>
<div class="container">
    <hr />
    <form asp-controller="NoteTypes" asp-action="Delete" asp-route-id="@(Model.UserName)" method="post">
        <div asp-validation-summary="All" class="text-danger"></div>
        @Html.AntiForgeryToken()
        <div class="form-actions no-color">
            <div class="nsg-float-left text-left">
                <span>
                    @Html.ActionLink("Back to List", "Index")
                </span>
            </div>
            <div class="nsg-float-right text-right">
                <span>
                    <strong>Are you sure you want to delete this User?</strong>
                    <input type="submit" value="Delete" class="btn btn-danger" />
                </span>
            </div>
            <div class="nsg-clear-both"></div>
        </div>

    </form>

</div>

@await Html.PartialAsync("UserDetailPartial", @Model)
